<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:type="MailApp">
  <Id>6d8a9c5b-84a6-4b2c-b645-7fdc898f1abc</Id>
  <Version>1.0.0.0</Version>

  <ProviderName DefaultValue="DIS OUH" />
  <DefaultLocale>en-US</DefaultLocale>

  <DisplayName DefaultValue="ChatGPT Email Assistant" />
  <Description DefaultValue="Help writing better emails with ChatGPT" />
  <IconUrl DefaultValue="https://jgramlange.github.io/office-addin-chatgpt/assets/icon.png" />
  <SupportUrl DefaultValue="https://jgramlange.github.io/office-addin-chatgpt/support.html" />

  <Hosts>
    <Host Name="Mailbox" />
  </Hosts>

  <Requirements>
    <Sets>
      <Set Name="Mailbox" MinVersion="1.1" />
    </Sets>
  </Requirements>

  <FormSettings>
    <Form xsi:type="ItemCompose">
      <DesktopSettings>
        <SourceLocation DefaultValue="https://jgramlange.github.io/office-addin-chatgpt/taskpane.html" />
      </DesktopSettings>
    </Form>
  </FormSettings>
</OfficeApp>
